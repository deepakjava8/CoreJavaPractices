package com.ust.employee;

public class Employee {
	private int id;
	private String name;
	private int salary;
	private String dept;
	
	public Employee() {
		
	}
	public Employee(int id, String name, int salary, String dept) {
		this.id = id;
		this.name = name;
		this.salary = salary;
		this.dept = dept;
	}
	
}
